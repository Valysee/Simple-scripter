game:GetService("ReplicatedStorage").RemoteEvents.CandyEvent:FireServer(unpack(args))
    
local args = {
    [1] = 0,
    [2] = "Zombie"
}

game:GetService("ReplicatedStorage").RemoteEvents.CandyEvent:FireServer(unpack(args))
    
local args = {
    [1] = 0,
    [2] = "Ghost"
}

game:GetService("ReplicatedStorage").RemoteEvents.CandyEvent:FireServer(unpack(args))
    
local args = {
    [1] = 0,
    [2] = "Pumpkin"
}

game:GetService("ReplicatedStorage").RemoteEvents.CandyEvent:FireServer(unpack(args))
    
local args = {
    [1] = 0,
    [2] = "Goblin"
}

game:GetService("ReplicatedStorage").RemoteEvents.CandyEvent:FireServer(unpack(args))
end)

