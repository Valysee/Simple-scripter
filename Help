local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wizard"))()

local PhantomForcesWindow = Library:NewWindow("simple script")

local KillingCheats = PhantomForcesWindow:NewSection("money")

KillingCheats:CreateTextbox("Strength and money", function(Value) 
 [1] = {
        ["Stage"] = Value
        ["Strength"] = "Value",
        ["Cash"] = "Value"
    }
}

game:GetService("ReplicatedStorage").RemoteEvents.Stats:FireServer(unpack(args))
end)

KillingCheats:CreateTextbox("Get Pumpkin", function(Value) 
  [1] = Value
}

game:GetService("ReplicatedStorage").RemoteEvents.PumpkinEvent:FireServer(unpack(args))
end)

KillingCheats:CreateTextbox("Get Candy", function(Value) 
 [1] = Value
}

game:GetService("ReplicatedStorage").RemoteEvents.trickOrTreatcandyEvent:FireServer(unpack(args))
end)

KillingCheats:CreateButton("best tool", function() 
  local args = {
    [1] = "1ST ULTIMATE SUPREME CLASS"
}

game:GetService("ReplicatedStorage").RemoteEvents.GiveTool:FireServer(unpack(args))
end)

KillingCheats:CreateButton("fast lifter (game pass + free)", function() 
local args = {
    [1] = "FastLifter"
}

game:GetService("ReplicatedStorage").GamePassCheckEvent:FireServer(unpack(args))
end)

